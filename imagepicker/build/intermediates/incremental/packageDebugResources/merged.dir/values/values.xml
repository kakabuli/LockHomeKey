<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="ic_back_press">#323336</color>
    <color name="ip_camera_pre">#353535</color>
    <color name="ip_color_accent">#1AAD19</color>
    <color name="ip_color_press">#ffe0e0e0</color>
    <color name="ip_color_primary">#393A3F</color>
    <color name="ip_color_primary_dark">#303135</color>
    <color name="ip_color_primary_trans">#ec2f3335</color>
    <color name="ip_text_primary_inverted">#ffffff</color>
    <color name="ip_text_secondary_inverted">#66ffffff</color>
    <declare-styleable name="CropImageView">
        <attr format="color" name="cropMaskColor"/>
        <attr format="color" name="cropBorderColor"/>
        <attr format="dimension" name="cropBorderWidth"/>
        <attr format="dimension" name="cropFocusWidth"/>
        <attr format="dimension" name="cropFocusHeight"/>
        <attr format="enum" name="cropStyle">
            <enum name="rectangle" value="0"/>
            <enum name="circle" value="1"/>
        </attr>
    </declare-styleable>
    <string name="cancel">cancel</string>
    <string name="ip_all_images">所有图片</string>
    <string name="ip_complete">完成</string>
    <string name="ip_folder_image_count">A total of %1$d piece</string>
    <string name="ip_origin">Original image</string>
    <string name="ip_origin_size">Original image(%1$s)</string>
    <string name="ip_photo_crop">照片裁剪</string>
    <string name="ip_play_camera">Taking pictures</string>
    <string name="ip_preview">preview</string>
    <string name="ip_preview_count">preview(%1$d)</string>
    <string name="ip_preview_image_count">%1$d/%2$d</string>
    <string name="ip_select_complete">complete(%1$d/%2$d)</string>
    <string name="ip_select_limit">Choose %1$d image at most</string>
    <string name="no_photo">A total of 0 zhang</string>
    <string name="not_allow_camera_warring">Permission is denied to open the camera</string>
    <string name="not_allow_sd_warring">Permission is disabled and local images cannot be selected</string>
    <string name="photo">照片</string>
    <string name="photograph">拍摄</string>
    <string name="prompt">prompt</string>
    <string name="select">choose</string>
    <string name="sure">sure</string>
    <string name="sure_delete_this_photo">Would you like to delete this photo</string>
    <style name="ImagePickerTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:fitsSystemWindows">true</item>
    </style>
    <style name="ImagePickerThemeFullScreen" parent="ImagePickerTheme">
        <item name="android:fitsSystemWindows">false</item>
    </style>
    <style name="SuperCheckboxTheme" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/selector_item_checked</item>
    </style>
    <style name="popupwindow_anim_style">
        <!-- 指定显示的动画xml -->
        <item name="android:windowEnterAnimation">@anim/show_from_bottom</item>
        <!-- 指定消失的动画xml -->
        <item name="android:windowExitAnimation">@anim/hide_to_bottom</item>
    </style>
</resources>